- url = query.persisted? ? admin_superjob_query_path(query) : admin_superjob_queries_path
= form_with model: query, url: url do |f|
  = f.label :title, 'Наименование запроса'
  = f.text_field :title, class: 'form-control'
  .invalid-feedback
  = f.fields_for :query_params, OpenStruct.new(query.query_params) do |qp|
    - query.query_params['keywords'].each do |kw|
      .form-group
        = hidden_field_tag 'super_job_query[query_params][keywords][][srws]', kw['srws'], class: 'form-control'
        = hidden_field_tag 'super_job_query[query_params][keywords][][skwc]', kw['skwc'], class: 'form-control'
        = label_tag 'super_job_query[query_params][keywords][][keys]', 'Включая слова'
        = text_field_tag 'super_job_query[query_params][keywords][][keys]', kw['keys'], class: 'form-control'
    .form-group
      = qp.label :exclude_words, 'Исключая слова'
      = qp.text_field :exclude_words, class: 'form-control'
      small.form-text.text-muted Пробел - условие И, запятая - условие ИЛИ
    h5 Резюме опубликованные за:
    .form-check.form-check-inline
      = qp.radio_button :period, 1, class: 'form-check-input'
      = qp.label :period, '24 часа', class: 'form-check-label', for: 'super_job_query_query_params_period_1'
    .form-check.form-check-inline
      = qp.radio_button :period, 3, class: 'form-check-input'
      = qp.label :period, '3 дня', class: 'form-check-label', for: 'super_job_query_query_params_period_3'
    .form-check.form-check-inline
      = qp.radio_button :period, 7, class: 'form-check-input'
      = qp.label :period, 'неделю', class: 'form-check-label', for: 'super_job_query_query_params_period_7'
    .form-check.form-check-inline
      = qp.radio_button :period, 14, class: 'form-check-input'
      = qp.label :period, '2 недели', class: 'form-check-label', for: 'super_job_query_query_params_period_14'
    .form-check.form-check-inline
      = qp.radio_button :period, 30, class: 'form-check-input'
      = qp.label :period, '1 месяц', class: 'form-check-label', for: 'super_job_query_query_params_period_30'
    .form-check.form-check-inline
      = qp.radio_button :period, 60, class: 'form-check-input'
      = qp.label :period, '2 месяца', class: 'form-check-label', for: 'super_job_query_query_params_period_60'
    h5.mt-3 Уровень дохода:
    .form-row.mt-3
      .col
          = qp.text_field :payment_from, class: 'form-control', placeholder: 'От'
      .col
          = qp.text_field :payment_to, class: 'form-control', placeholder: 'До'
    h5.mt-3 Возраст:
    .form-row.mt-3
      .col
          = qp.text_field :age_from, class: 'form-control', placeholder: 'От'
      .col
          = qp.text_field :age_to, class: 'form-control', placeholder: 'До'
    h5.mt-3 Местоположение:
    .form-row.mt-3
      .col
        = label_tag 'region', 'Область'
        = select_tag 'super_job_query[query_params][o][]', nil, class: 'form-control browser-default regions', prompt: 'Введите область', multiple: true
      .col
        = label_tag 'town', 'Город'
        = select_tag 'super_job_query[query_params][t][]', nil, class: 'form-control browser-default towns', prompt: 'Введите город', multiple: true
    .form-check.mt-3
      = qp.check_box :moveable, class: 'form-check-input'
      = qp.label :moveable, 'Готов к переезду', class: 'form-check-label'
      = qp.hidden_field :experience

    h5.mt-3 Отрасли:
    .form-row.mt-3
      .col
        = select_tag 'super_job_query[query_params][catalogues][]', nil, class: 'form-control browser-default catalogues', prompt: 'Введите отрасль', multiple: true
  br
  = f.submit 'Сохранить'

